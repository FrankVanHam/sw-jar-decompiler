_package deco
$

_pragma(classify_level=restricted, topic={jar_decompiler})
def_slotted_exemplar(:jpn_method,
	{
		{:args, _unset},
		{:body, _unset}
	},
	:jpn_base)
$

_pragma(classify_level=restricted, topic={jar_decompiler})
_method jpn_method.init(p_name, p_args, p_body)
	## 
	.args << p_args
	.body << p_body
	>> _super.init(p_name)
_endmethod
$

