_package deco
$

#remex(:jpn_cast)

_pragma(classify_level=restricted, topic={jar_decompiler})
def_slotted_exemplar(:jpn_cast,
	{
		{:lex_value, _unset},
		{:expression, _unset, :readable}
	},
	:jpn_base)
$

_pragma(classify_level=restricted, topic={jar_decompiler})
_method jpn_cast.init(p_lex_name, p_exp)
	.lex_value << p_lex_name
	.expression << p_exp
	>> _super.init()
_endmethod
$
