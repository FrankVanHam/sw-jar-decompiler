_package deco
$

#remex(:jpn_file)

_pragma(classify_level=restricted, topic={jar_decompiler})
def_slotted_exemplar(:jpn_file,
	{
		{:content, _unset, :readable}
	},
	:jpn_base)
$
_pragma(classify_level=restricted, topic={jar_decompiler})
_method jpn_file.init(p_name)
	##
	.content << rope.new()
	>> _super.init(p_name)
_endmethod
$

_pragma(classify_level=restricted, topic={jar_decompiler})
_method jpn_file.add(p_node)
	.content.add(p_node)
_endmethod
$
